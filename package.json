{
  "name": "MubasherTrade",
  "version": "0.1.0",
  "description": "MubasherTrade Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/mubasher_trade.icns --prune=true --out=release-builds-mac --appBundleId=com.rubix.desktop --osxSign=true",
    "package-win": "electron-packager . MubasherTrade --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/windows/mubasher_trade.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"MubasherTrade\"",
    "create-dmg": "node ./createDMG.js",
    "mac-appstore": "electron-packager . --overwrite --asar=true --platform=mas --arch=x64 --icon=assets/icons/mac/mubasher_trade.icns --prune=true --out=release-builds-mac --appBundleId=com.rubix.desktop --osxSign=true"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Mubasher",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-packager": "^12.1.2"
  }
}
